{
    "$schema": "https://json.schemastore.org/package.json",
    "name": "@siyuan-community/siyuan-sdk",
    "version": "0.3.11",
    "packageManager": "pnpm@9.1.3",
    "engines": {
        "node": "22"
    },
    "description": "A simple and easy to use SDK for SiYuan.",
    "keywords": [
        "api",
        "sdk",
        "siyuan",
        "siyuan-note",
        "siyuan-community"
    ],
    "homepage": "https://github.com/siyuan-community/siyuan-sdk/tree/main/node",
    "bugs": {
        "url": "https://github.com/siyuan-community/siyuan-sdk/issues"
    },
    "license": "AGPL-3.0-or-later",
    "repository": {
        "type": "git",
        "url": "https://github.com/siyuan-community/siyuan-sdk.git",
        "directory": "node"
    },
    "author": {
        "name": "Zuoqiu Yingyi",
        "url": "https://github.com/Zuoqiu-Yingyi"
    },
    "contributors": [
        {
            "name": "SiYuan Community",
            "email": "zuoez02@siyuan-note.club",
            "url": "https://github.com/siyuan-community"
        }
    ],
    "funding": [
        {
            "type": "Ko-fi",
            "url": "https://ko-fi.com/zuoqiu"
        },
        {
            "type": "AFDIAN",
            "url": "https://afdian.net/a/zuoqiu"
        }
    ],
    "main": "./dist/index.umd.cjs",
    "types": "./dist/index.d.ts",
    "module": "./dist/index.js",
    "browser": "./dist/index.iife.js",
    "type": "module",
    "private": false,
    "publishConfig": {
        "access": "public",
        "registry": "https://registry.npmjs.org/"
    },
    "files": [
        "dist",
        "src",
        "package.json",
        "README.md",
        "README-zh-Hans.md",
        "LICENSE"
    ],
    "scripts": {
        "prepare": "cd ./.. && husky ./node/.husky",
        "dev": "node --import=./scripts/ts-node-register.js ./scripts/dev.ts",
        "test": "vitest",
        "test:ui": "vitest --ui",
        "test:once": "vitest run",
        "build": "vite build",
        "build:prebuild": "node --import=./scripts/ts-node-register.js ./scripts/prebuild.ts",
        "publish:npm": "run-s build:prebuild build && npm publish",
        "format": "run-s format:eslint format:prettier",
        "format:eslint": "eslint --fix ./src",
        "format:prettier": "run-s format:prettier:schemas format:prettier:code",
        "format:prettier:schemas": "prettier -w \"**/*.json5\" ./../schemas",
        "format:prettier:code": "prettier -w \"**/*.{js,ts}\"",
        "spell": "run-s spell:code spell:schemas",
        "spell:code": "cspell .",
        "spell:schemas": "cspell ./../schemas",
        "lint": "eslint ./src",
        "coverage": "vitest run --coverage",
        "check": "run-s spell lint format coverage"
    },
    "dependencies": {
        "axios": "^1.7.2",
        "isomorphic-ws": "^5.0.0",
        "js-base64": "^3.7.7",
        "ofetch": "^1.3.4"
    },
    "devDependencies": {
        "@eslint/js": "^9.4.0",
        "@ianvs/prettier-plugin-sort-imports": "^4.2.1",
        "@ltd/j-toml": "^1.38.0",
        "@nodelib/fs.walk": "^2.0.0",
        "@types/node": "^20.14.1",
        "@vitest/coverage-istanbul": "^1.6.0",
        "@vitest/ui": "^1.6.0",
        "ajv": "^8.16.0",
        "ajv-formats": "^3.0.1",
        "chokidar": "^3.6.0",
        "cspell": "^8.8.4",
        "dotenv": "^16.4.5",
        "eslint": "^9.4.0",
        "husky": "^9.0.11",
        "json5": "^2.2.3",
        "npm-run-all": "^4.1.5",
        "path-browserify": "^1.0.1",
        "prettier": "^3.3.0",
        "prettier-plugin-packagejson": "^2.5.0",
        "quicktype-core": "^23.0.170",
        "ts-node": "^10.9.2",
        "typescript": "^5.4.5",
        "typescript-eslint": "^7.12.0",
        "vite": "^5.2.12",
        "vite-plugin-dts": "^3.9.1",
        "vite-plugin-static-copy": "^1.0.5",
        "vitest": "^1.6.0",
        "ws": "^8.17.0"
    }
}
